{
  "name": "test-cluster",
  "infrastructure": [
    {
      "id": "aws_infra",
      "kind": "vm",
      "provider": "aws",
      "region": "us-west-2"
    }
  ],
  "nodes": [
    {
      "id": "node1",
      "infrastructure_id": "aws_infra",
      "properties": {
        "instance_type": "t2.micro"
      },
      "remote_access": {
        "public_ssh_key": "/path/to/key.pub"
      },
      "dns": {
        "high_level_domain": "example.com"
      }
    }
  ],
  "devcontainers": [
    {
      "id": "dev1",
      "node_id": "node1",
      "source": {
        "url": "https://github.com/example/repo.git"
      },
      "remote_access": {
        "ssh": {
          "port": 2222
        },
        "openvscode_server": {
          "port": 8080
        }
      }
    }
  ]
}
